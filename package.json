{
  "name": "graphile-scheduler",
  "version": "0.0.1",
  "description": "Job scheduler for PostgreSQL",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepack": "rm -Rf dist && tsc",
    "watch": "mkdir -p dist && tsc --watch",
    "lint": "eslint 'src/**/*'",
    "test": "createdb graphile_scheduler_test || true && psql -Xv ON_ERROR_STOP=1 -f __tests__/reset-db.sql graphile_scheduler_test && jest -i"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davbeck/graphile-scheduler.git"
  },
  "keywords": [
    "postgresql",
    "postgres",
    "pg",
    "scheduler",
    "sql",
    "easy",
    "fast",
    "jobs",
    "tasks"
  ],
  "author": "David Beck <code@davidbeck.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davbeck/graphile-scheduler/issues"
  },
  "homepage": "https://github.com/davbeck/graphile-scheduler#readme",
  "dependencies": {
    "@types/debug": "^4.1.2",
    "@types/jest": "^24.0.11",
    "@types/pg": "^7.4.11",
    "graphile-worker": "0.4.0",
    "moment": "^2.24.0",
    "pg": ">=6.5 <8",
    "pg-connection-string": "^2.0.0",
    "tslib": "^1.9.3",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/sinon": "^7.5.2",
    "@types/yargs": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint_d": "^8.0.0",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "sinon": "^9.0.0",
    "ts-jest": "^24.0.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.3.3333"
  },
  "files": [
    "dist",
    "sql"
  ],
  "engines": {
    "node": ">=10.0.0"
